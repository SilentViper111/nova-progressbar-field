{
    "name": "signifly/nova-progressbar-field",
    "description": "A Laravel Nova progress bar field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Signifly\\Nova\\Cards\\ProgressBar\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Signifly\\Nova\\Cards\\ProgressBar\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}