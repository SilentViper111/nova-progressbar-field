{
    "name": "signifly/nova-progressbar-field",
    "description": "A Laravel Nova progress bar field.",
    "keywords": [
        "laravel",
        "nova",
        "progressbar",
        "fields",
    ],
    "license": "MIT",
    "authors": [{
        "name": "Ro Kleine Sonne",
        "email": "ro@signifly.com",
        "role": "Developer"
    }],
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Signifly\\Nova\\Fields\\ProgressBar\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Signifly\\Nova\\Fields\\ProgressBar\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}